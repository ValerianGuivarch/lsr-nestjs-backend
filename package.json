{
  "name": "auth-sample",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "checkdeps": "ncu",
    "checkdeps:apply": "ncu -u",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "npm run test:watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/test/e2e/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/static": "^6.10.2",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.4.2",
    "@prismicio/client": "^7.1.0",
    "@types/jsdom": "^21.1.1",
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fluent-json-schema": "^4.1.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "mongoose": "^7.0.3",
    "mongoose-paginate-v2": "^1.7.1",
    "npm-check-updates": "^16.10.7",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.1",
    "rxjs": "7.8.0",
    "twilio": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.19.0",
    "@nestjs/cli": "9.3.0",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/chance": "^1.1.3",
    "@types/ejs": "^3.1.2",
    "@types/express": "4.17.17",
    "@types/fastify-multipart": "^0.7.0",
    "@types/jest": "29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.15.11",
    "@types/qs": "^6.9.7",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "chance": "^1.1.11",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.3",
    "is-ci": "^3.0.1",
    "jest": "29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-when": "^3.5.2",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.10.7",
    "prettier": "2.8.7",
    "prettier-package-json": "^2.8.0",
    "sonarqube-scanner": "^3.0.1",
    "supertest": "6.3.3",
    "testcontainers": "^9.4.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.15 <19.0.0",
    "npm": ">=9 <10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --fix",
    "*.ts": "prettier --config .prettierrc.json --write src/**/*.ts",
    "package.json": [
      "prettier-package-json --write package.json .eslintrc.json .ncurc.json .prettierrc.json",
      "git add"
    ]
  }
}
